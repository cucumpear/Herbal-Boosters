<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RecipeDef>
    <defName>MakeTenHerbalBoosters</defName>
    <label>make ten herbal immunity boosters</label>
    <description>Make ten herbal immunity boosters.</description>
    <jobString>Making ten herbal immunity boosters.</jobString>
	<workSkillLearnFactor>1</workSkillLearnFactor>
    <workSpeedStat>DrugProductionSpeed</workSpeedStat>
    <effectWorking>Tailor</effectWorking>
    <soundWorking>Recipe_Brewing</soundWorking>
    <allowMixingIngredients>true</allowMixingIngredients>
    <workAmount>4000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>HerbalMedicine</li>
          </thingDefs>
        </filter>
        <count>20</count>
      </li>
	  <li>
        <filter>
          <thingDefs>
            <li>RawBerries</li>
          </thingDefs>
        </filter>
        <count>50</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>HerbalMedicine</li>
		<li>RawBerries</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <HerbalImmunityBooster>10</HerbalImmunityBooster>
    </products>
	<skillRequirements>
        <li>
          <skill>Medicine</skill>
          <minLevel>2</minLevel>
        </li>
        <li>
          <skill>Crafting</skill>
          <minLevel>2</minLevel>
        </li>
    </skillRequirements>
	<researchPrerequisite>MedicineProduction</researchPrerequisite>
    <workSkill>Medicine</workSkill>
	<recipeUsers>
		<li>DrugLab</li>
	</recipeUsers>
  </RecipeDef>

